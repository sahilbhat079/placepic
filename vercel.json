{
    "version": 2,
    "builds":[{"src":"./index.js*", "use":"@vercel/node"}],
    "routes": [
      { "handle": "filesystem" },
      { "src": "/(.*)", "dest": "/" },
      { "src": "/places", "dest": "/index.js" },
      { "src": "/user-places", "methods": ["GET", "PUT"], "dest": "/index.js" },
      { "src": "^/$", "dest": "/index.js" }
    ],
    "rewrites": [{ "source": "/images/(.*)", "destination": "/images/$1" }],
    "functions": { "*.js": { "runtime": "nodejs12" } }
  }



