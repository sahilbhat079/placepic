{
  "version": 2,
  "routes": [
    { "handle": "filesystem" },
    {  "src": "/api/places", "methods": ["GET"] },
    {  "src": "/api/user-places", "methods": ["GET", "PUT"] },
    { "src": "/images/(.*)", "dest": "/public/images/$1" } 
  ],
  "builds": [
    { "src": "api/places.js", "use": "@vercel/node" },
    { "src": "api/user-places.js", "use": "@vercel/node" }
  ]
}