{
  "version": 2,
  "builds": [
    { "src": "/api/places/**", "use": "@vercel/node" },
    { "src": "/api/user-places/**", "use": "@vercel/node" }
  ],
  "routes": [
    { "src": "/(.*)", "dest": "/index.js" },
    { "src": "/api/places/(.*)", "dest": "/api/places.js" },
    { "src": "/api/user-places/(.*)", "dest": "/api/user-places.js" },
    { "src": "/images/(.*)", "dest": "/public/images/$1" }
  ]
}